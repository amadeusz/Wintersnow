<h1>Genotypy</h1>

<p id="notice"><%= notice %></p>

<% @genotypes.each do |@genotype| %>
	<div>
		<%= form_for(@genotype) do |f| %>
		<%= f.text_field :genotyp %> <% if false %><%= f.submit '', :class => 'zapisz' %><% end %>
		<% end %>
		<%= link_to image_tag('icons/destroy.png'), @genotype, :confirm => 'Are you sure?', :method => :delete %>
	</div>
<% end %>

<%= form_for(@new_genotype) do |f| %>
	<div><%= f.text_field :genotyp %><% if false %> <%= f.submit '', :class => 'dodaj' %><% end %></div>
<% end %>
