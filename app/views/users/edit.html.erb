<h1>Editing user</h1>

<% form_for(@user) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :klucz %><br />
    <%= f.text_field :klucz %>
  </p>
  <p>
    <%= f.label :obserwowane %><br />
    <%= f.text_area :obserwowane %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @user %> |
<%= link_to 'Back', users_path %>
